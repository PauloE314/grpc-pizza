{
  "name": "tomato",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.js | pino-pretty",
    "start": "node ./src/index.js",
    "build": "mkdir ./src/pb/codegen -p && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/pb/codegen --grpc_out=grpc_js:./src/pb/codegen -I=./src/pb/ ./src/pb/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-pino-logger": "^7.0.0",
    "google-protobuf": "^3.20.1",
    "grpc": "^1.24.11",
    "grpc-tools": "^1.11.2",
    "pino": "^7.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/node": "^17.0.35",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.16",
    "pino-pretty": "^7.6.1"
  }
}
