{
  "name": "tomato",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node ./src/index.js",
    "knex": "knex --knexfile ./src/db/knexfile.js",
    "migrate": "knex --knexfile ./src/db/knexfile.js migrate:latest",
    "build": "mkdir ./src/pb/codegen -p && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/pb/codegen --grpc_out=grpc_js:./src/pb/codegen -I=./src/pb/ ./src/pb/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "google-protobuf": "^3.20.1",
    "grpc-tools": "^1.11.2",
    "knex": "^2.0.0",
    "nodemon": "^2.0.16",
    "pino": "^7.11.0",
    "sqlite3": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/knex": "^0.16.1",
    "@types/node": "^17.0.35",
    "dotenv": "^16.0.1"
  }
}
